{
  "name": "Tabbed",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Grabs your tabs and stores them in a Tabbed server.",
  "homepage_url": "http://dreamintech.net/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "tabs"
  ]
}
